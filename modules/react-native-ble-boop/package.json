{
  "name": "react-native-ble-boop",
  "version": "1.0.0",
  "description": "Native BLE Turbo Module for boop discovery",
  "main": "lib/commonjs/index.js",
  "types": "lib/typescript/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "ble",
    "bluetooth",
    "turbo-module"
  ],
  "author": "boop",
  "license": "MIT",
  "devDependencies": {
    "@react-native/codegen": "^0.74.0",
    "react-native-builder-bob": "^0.23.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNBleBoopSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  }
}
